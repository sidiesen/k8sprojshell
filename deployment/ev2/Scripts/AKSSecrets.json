{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "clusterName": {
        "type": "string"
      },
      "keyVaultName": {
        "type": "string"
      },
      "AKSConfigSecretName": {
        "type": "string"
      }
    },
    "resources": [
      {
        "type": "Microsoft.KeyVault/vaults/secrets",
        "name": "[concat(parameters('keyVaultName'),'/', parameters('AKSConfigSecretName'))]",
        "apiVersion": "2015-06-01",
        "properties": {
          "value": "[Base64ToString(ListCredential(resourceId('Microsoft.ContainerService/managedClusters/accessProfiles', parameters('clusterName'),'clusterAdmin'), '2017-08-31').properties.kubeConfig)]"
        }
      }
    ]
}
    
  
  
  
  